<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.robust_binaries.paperback_harbor_app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Paperbackharborapp">
       <activity
            android:name=".PublisherDashboard"
            android:exported="false" />
        <activity
            android:name=".PublisherContent"
            android:exported="false" />
        <activity
            android:name=".Admin"
            android:exported="false" />
        <activity
            android:name=".BookCard"
            android:exported="false" />
        <activity
            android:name=".AuthorListActivity"
            android:exported="false" />

        <activity
            android:name=".MainActivity"
            android:exported="true"/>
           <activity android:name=".AuthenticateUserData"></activity>
        <activity android:name=".ForgetPasswordSuccessMessage" />
        <activity android:name=".SetNewPassword" />
        <activity android:name=".ForgetPasswordCheckOTP" />
        <activity android:name=".ForgetPassword" />
        <activity android:name=".SignUp" />
        <activity android:name=".Login" />
        <activity android:name=".StartUpScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>